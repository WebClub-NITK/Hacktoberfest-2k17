#include<stdio.h>
void main(){
    int a[100],n,i,j;
    printf("enter number of elements:");
    scanf("%d",&n);
    printf("enter array elements:");
    for(i=0;i<n;i++)
        scanf("%d",&a[i]);
    for(i=0;i<n-1;i++){
        for(j=0;j<n-i-1;j++){
            if(a[j+1]<a[j]){
                int t=a[j];
                a[j]=[j+1];
                a[j+1]=t;
            }
        }
    }
    printf("sorted array: ");
    for(i=0;i<n;i++){
        printf("%d  ",a[i]);
    }
}
